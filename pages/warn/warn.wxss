/* pages/warn/warn.wxss */
.container{
  width:100%;
  font-size: 30rpx;
}
.warntitle , .pictitle{
  width: 100%;
  background-color: #eee;
  padding: 30rpx 50rpx;
}
.warntype{
  width: 100%;
 
}
.warntype .warnchoose{
  padding: 50rpx;
   display: flex;
  flex-wrap: wrap; 
  justify-content: space-around; 
}
 .warntype .warnbox{
   font-size: 40rpx;
   text-align: center;
   line-height: 100rpx;
  width: 45%;
  height: 100rpx;
  border: 2rpx solid #b9dd08;
  margin-top: 30rpx;
  border-radius: 6rpx;
} 
.warntype .warnbox:nth-of-type(1){
   margin-top: 0rpx;
}
.warntype .warnbox:nth-of-type(2){
   margin-top: 0rpx;
}
.picwrap{
  width: 100%;
  display: flex;
   flex-wrap: wrap; 
   padding-bottom: 40rpx;

}
.pic , .piccontainer{
  border: 1rpx solid #eee;
  margin-left:60rpx;
  margin-top:40rpx;
  width: 120rpx;
  height: 120rpx;
  
}
.pic image{
  width: 100%;
  height: 100%;
  overflow: visible;
  position: relative;
}
.pic image icon{
  position: absolute;
  top:-20rpx;
  right:-20rpx;
}
.piccontainer{
  background-color: #eee;
  text-align: center;
  line-height: 120rpx;
  font-size: 20rpx;
}
.inp-top{
  width: 100%;
  height: 25rpx;
  background-color: #eee;
}
.inp  input{
  font-size: 35rpx;
  padding:20rpx 0;
  background-color: #fff;
  margin:0rpx 30rpx;
}
.inp input:nth-of-type(1){
  border-bottom: 2px solid #eee; 
}
.btn button{
  color: #666;
}


